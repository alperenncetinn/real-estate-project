# Database Connection (Supabase PostgreSQL)
DB_CONNECTION=Host=your-host.pooler.supabase.com;Port=5432;Database=postgres;Username=postgres.your-project-ref;Password=YOUR_PASSWORD;SSL Mode=Require;Trust Server Certificate=true

